                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentTop="true"
                   android:layout_marginStart="24dp"
                   android:layout_marginTop="38dp"
                   android:layout_marginEnd="8dp"
                   android:text="Profile"
                   android:textColor="@color/light_bg_dark_primary_text"
                   android:textSize="15sp"
                   android:textStyle="bold" />

               <TextView
                   android:id="@+id/textView3s"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_below="@+id/akunTxts"
                   android:layout_marginStart="24dp"
                   android:layout_marginTop="30dp"
                   android:layout_marginEnd="8dp"
                   android:text="Nama"
                   android:textSize="12sp" />

               <View
                   android:id="@+id/profile_names"
                   android:layout_width="240dp"
                   android:layout_height="20dp"
                   android:background="@drawable/shimer_bg"
                   android:layout_below="@+id/textView3s"
                   android:layout_marginStart="24dp"
                   android:layout_marginTop="6dp"
                   android:layout_marginEnd="8dp"
                   android:textColor="@color/common_google_signin_btn_text_dark_focused" />

               <View
                   android:id="@+id/view3s"
